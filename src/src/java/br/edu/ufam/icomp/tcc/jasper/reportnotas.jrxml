	<queryString>
		<![CDATA[SELECT
     usuario."tx_nome" AS usuario_tx_nome,
     tcctcc."titulo" AS tcctcc_titulo,
     tccnotas."nota1" AS tccnotas_nota1,
     tccnotas."nota2" AS tccnotas_nota2,
     tccnotas."nota3" AS tccnotas_nota3
FROM
     "public"."tcctcc" tcctcc INNER JOIN "public"."tccnotas" tccnotas ON tcctcc."id" = tccnotas."id_tcc_tcc"
     INNER JOIN "public"."aluno" aluno ON tcctcc."id_aluno" = aluno."id_aluno"
     INNER JOIN "public"."usuario" usuario ON aluno."id_usuario" = usuario."id_usuario"
WHERE
     tcctcc."id_periodo" = $P!{periodo}
ORDER BY
     usuario."tx_nome" ASC]]>
	</queryString>
