	<parameter name="imgBrasao" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
     usuario."tx_nome" AS usuario_tx_nome,
     curso."tx_curso" AS curso_tx_curso,
     tcctcc."titulo" AS tcctcc_titulo,
     usuario_A."tx_nome" AS usuario_A_tx_nome,
     tccworkshop."avaliador1" AS tccworkshop_avaliador1,
     tccworkshop."avaliador2" AS tccworkshop_avaliador2,
     tccworkshop."data" AS tccworkshop_data
FROM
     "public"."tcctcc" tcctcc INNER JOIN "public"."tccworkshop" tccworkshop ON tcctcc."id" = tccworkshop."id_tcc_tcc"
     INNER JOIN "public"."aluno" aluno ON tcctcc."id_aluno" = aluno."id_aluno"
     INNER JOIN "public"."professor" professor ON tcctcc."id_professor" = professor."id"
     INNER JOIN "public"."usuario" usuario_A ON professor."id_usuario" = usuario_A."id_usuario"
     INNER JOIN "public"."usuario" usuario ON aluno."id_usuario" = usuario."id_usuario"
     INNER JOIN "public"."curso" curso ON aluno."curso_id" = curso."id"
WHERE
     tcctcc."id" = $P!{idtcc}]]>
	</queryString>
